package dega.gui{	import flash.display.Sprite;		public class IconValidator extends Sprite {		private var sign:Sprite;		public function IconValidator () {			sign = new Sprite();			addChild (sign);		}		public function negative ():void {			// X sign			sign.graphics.clear ();			sign.graphics.lineStyle (1,0xFF3265);			sign.graphics.lineTo (7,7);			sign.graphics.moveTo (0,7);			sign.graphics.lineTo (7,0);		}		public function positive ():void {			// well Done Sign			sign.graphics.clear ();			sign.graphics.lineStyle (1,0x00FF00);			sign.graphics.moveTo (0,5);			sign.graphics.lineTo (3,7);			sign.graphics.lineTo (8,0);			addChild (sign);		}	}}