package dega.utils {
	
	public class RandomCharacter {
		
		public static function getRandomCharacter ( index:int = 0 ):String {
			var a:String = "abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ";
			var alphabet:Array = a.split("");
			var indexChar:int = index > a.length? a.length - 1: index;
			return alphabet[ indexChar ].toString();
		}
	}
}